<h1>Welcome to SuperFeed!</h1>
<h4>Your Feeds </h4>
<% if @github_response %>
  <h4>Github Feed</h4>
  <ul>
    <% @response.each do |item| %>
      <li>
        <img src="<%= item["owner"]["avatar_url"] %>" class="avatar_pic" />
        <%= Time.parse(item["updated_at"]).strftime("%b %d at %I:%M%p" ) %> |
        <%= item["name"] %></li>
    <% end %>
    <li><%= link_to "Disconnect your Github", "/auth/github" %></li>
  </ul>
<% else %>
  <%= link_to "Add your Github", "/auth/github" %>
<% end %>
<br>
<%= link_to "Add your LinkedIn", "/auth/linkedin" %>
<br>
<%= link_to "Add your Twitter", "/auth/twitter" %>
